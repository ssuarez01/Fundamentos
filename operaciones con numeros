#include <iostream>

using namespace std;

int main()
{
    int a , b , c, d, e, f , g;
    cout<< "ingresar primer operando: ";
    cin>> a;
    cout<< "ingresar segundo operando: ";
    cin>> b;
    c=a+b;
    cout<< "la suma es "<<c<<endl;
    d=a-b;
    cout<< "la resta es "<<d<<endl;
    e=a*b;
    cout<< "el producto es "<<e<<endl;
    g=a/b;
    cout<< "la division es "<<g<<endl;
    f=a%b;
    cout<< "el modulo es "<<f<<endl;
    
    
    
    return 0;
}
